#include<iostream>
using namespace std;

int* findDisappearedNumbers(int* nums, int numsSize, int* returnSize) {
  int *a=(int *)(numsSize*sizeof(int));
  int *b=(int *)(returnSize*sizeof(int));
  int i,j=0;

  for(i=0;i<numsSize;i++)
  {
      a[nums[i]]=1;

  }
  for(i=0;i<numsSize;i++)
  {
     if(a[nums[i]==0)
     b[j++]=a[nums[i]];



  }
  return b;




}
